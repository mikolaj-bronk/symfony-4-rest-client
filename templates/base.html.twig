<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block head_css %}
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
        {% endblock %}
    </head>
    <body>
    <div style="margin-top: 20px; margin-bottom: 20px;">
        <a href="{{ path('item_create') }}" class="btn btn-primary">Create</a>
        <a href="{{ path('items_all') }}" class="btn btn-primary">Show all</a>
        <a href="{{ path('items_available') }}" class="btn btn-primary">Show in stock</a>
        <a href="{{ path('items_unavailable') }}" class="btn btn-primary">Show out of stock</a>
        <a href="{{ path('items_greater_than', { 'amount': 5 }) }}" class="btn btn-primary">Amount greater than 5</a>
    </div>

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
